{
    "name": "radioactivehamster/dashify-keys",
    "description": "Convert underscores in an array's properties to hyphens.",
    "type": "library",
    "require": {
        "php": ">=7",
        "tightenco/collect": "^5.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Benner",
            "email": "james.benner@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RadHam\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpspec/phpspec": "^3.4",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "scripts": {
        "fix": "vendor/bin/php-cs-fixer fix",
        "test": "vendor/bin/phpspec run"
    }
}
